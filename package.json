{
  "name": "mobile-saas-starter",
  "version": "1.0.0",
  "description": "Production-ready mobile SaaS starter kit with React Native, Supabase, and Stripe",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "dev": "expo start",
    "ios": "expo start --ios",
    "android": "expo start --android",
    "web": "expo start --web",
    "eject": "expo eject",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build": "eas build --platform all",
    "build:preview": "eas build --platform all --profile preview",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "submit": "eas submit --platform all",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\""
  },
  "dependencies": {
    "expo": "~51.0.0",
    "expo-router": "~3.5.0",
    "expo-notifications": "~0.27.0",
    "expo-secure-store": "~13.0.0",
    "expo-local-authentication": "~15.0.0",
    "expo-dev-client": "~3.5.0",
    "expo-image": "~1.12.0",
    "react": "18.3.1",
    "react-native": "0.74.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "~3.31.0",
    "@react-native-async-storage/async-storage": "^1.23.0",
    "@supabase/supabase-js": "^2.43.0",
    "@stripe/stripe-react-native": "^14.2.0",
    "nativewind": "^2.0.11",
    "tailwindcss": "^3.4.0",
    "clsx": "^2.0.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@types/react": "~18.2.45",
    "@types/react-native": "~0.73.0",
    "typescript": "^5.3.0",
    "prettier": "^3.1.0",
    "eslint": "^8.55.0",
    "eslint-config-expo": "^6.1.0"
  },
  "private": true,
  "keywords": [
    "react-native",
    "expo",
    "saas",
    "starter",
    "kit",
    "supabase",
    "stripe",
    "typescript"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/mobile-saas-starter.git"
  },
  "author": "Your Name",
  "bugs": {
    "url": "https://github.com/yourusername/mobile-saas-starter/issues"
  },
  "homepage": "https://github.com/yourusername/mobile-saas-starter#readme"
}
